{
  "name": "todo-app-backend",
  "version": "1.0.0",
  "description": "Todo App Backend",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itayeylath/to-do-app-backend.git"
  },
  "keywords": [
    "todo",
    "express",
    "typescript",
    "mongodb",
    "firebase"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/itayeylath/to-do-app-backend/issues"
  },
  "homepage": "https://github.com/itayeylath/to-do-app-backend#readme",
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.0.3",
    "firebase-admin": "^11.11.0",
    "cors": "^2.8.5",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/jest": "^29.5.3",
    "@types/cors": "^2.8.17",
    "@types/winston": "^2.4.4",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  }
}
